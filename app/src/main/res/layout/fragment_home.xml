<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
   >
    <com.zyl_android.tenderinfo.project.builder.ObservableScrollView
        android:id="@+id/scrollview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/banner_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <com.zyl_android.generalutils.coustomview.MyViewPager
                    android:id="@+id/viewpager"
                    android:layout_width="match_parent"
                    android:layout_height="218dp" />

                <LinearLayout
                    android:id="@+id/group_contain"
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:layout_alignBottom="@id/viewpager"
                    android:gravity="center"
                    android:orientation="horizontal" />
            </RelativeLayout>
            <LinearLayout
                android:id="@+id/platform_linearLayout"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_below="@id/banner_layout"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="-20dp"
                android:background="@mipmap/hbg"
                android:orientation="horizontal"
                android:paddingBottom="4dp">

                <LinearLayout
                    android:id="@+id/bibiPlatform"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="57dp"
                        android:layout_height="57dp"
                        android:src="@mipmap/shouye_bibi" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/companyPlatform"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageView3"
                        android:layout_width="57dp"
                        android:layout_height="57dp"
                        android:src="@mipmap/shouye_qy" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/finacePlatform"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="57dp"
                        android:layout_height="57dp"
                        android:src="@mipmap/shouye_jr" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/servicePlatform"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageView"
                        android:layout_width="57dp"
                        android:layout_height="57dp"
                        android:src="@mipmap/shouye_fwzx" />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@id/platform_linearLayout"
                android:layout_marginLeft="12dp"
                android:layout_marginRight="12dp"
                android:layout_marginTop="4dp"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="83dp"
                    android:background="@mipmap/hbg2"
                    android:orientation="horizontal">

                    <RelativeLayout
                        android:id="@+id/projcetInfo_layout"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1.1"
                        android:clickable="true"
                        android:gravity="center">
                        <ImageView
                            android:id="@+id/projectImage"
                            android:layout_width="54dp"
                            android:layout_height="54dp"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="4dp"
                            android:src="@mipmap/xiangmu"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/projectImage"
                            android:layout_centerHorizontal="true"
                            android:text="项目信息"
                            android:textColor="#FFFFFF"
                            android:textSize="14sp"
                            android:textStyle="bold" />
                    </RelativeLayout>
                    <View
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="8dp"
                        android:layout_marginTop="8dp"
                        android:background="@color/homenew_block_line"/>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1.5"
                        android:orientation="vertical">
                        <TextView
                            android:id="@+id/niProject_text"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="拟在建项目"
                            android:textColor="@color/white"
                            android:textSize="13sp"
                            android:textStyle="bold"
                            />
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginLeft="10dp"
                            android:background="@color/homenew_block_line"/>

                        <TextView
                            android:id="@+id/pProject_text"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="PPP项目"
                            android:textColor="@color/white"
                            android:textSize="13sp"
                            android:textStyle="bold"
                            />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical">
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginBottom="6dp"
                            android:layout_marginTop="8dp"
                            android:layout_weight="1"
                            android:background="@color/homenew_block_line"/>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="@color/homenew_block_line"/>
                        <View
                            android:layout_width="1dp"
                            android:layout_height="0dp"
                            android:layout_marginBottom="8dp"
                            android:layout_marginTop="6dp"
                            android:layout_weight="1"
                            android:background="@color/homenew_block_line"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginRight="5dp"
                        android:layout_weight="1.5"
                        android:orientation="vertical">
                        <TextView
                            android:id="@+id/hostProject_text"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="业主委托项目"
                            android:textColor="@color/white"
                            android:textSize="13sp"
                            android:textStyle="bold"
                            />
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginRight="10dp"
                            android:background="@color/homenew_block_line"/>
                        <TextView
                            android:id="@+id/projcetInfo_text"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="项目信息订阅"
                            android:textColor="@color/white"
                            android:textSize="13sp"
                            android:textStyle="bold"
                            />
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="83dp"
                    android:layout_marginTop="10dp"
                    android:background="@mipmap/hbg3"
                    android:orientation="horizontal">

                    <View
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="8dp"
                        android:layout_marginTop="8dp"
                        android:background="@color/homenew_block_line"/>

                    <RelativeLayout
                        android:id="@+id/tenderProjcetInfo_layout"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1.1"
                        android:clickable="true"
                        android:gravity="center">

                        <ImageView
                            android:id="@+id/tenderProjectImage"
                            android:layout_width="54dp"
                            android:layout_height="54dp"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="4dp"
                            android:src="@mipmap/zhaobao" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/tenderProjectImage"
                            android:layout_centerHorizontal="true"
                            android:layout_marginBottom="2dp"
                            android:text="招标信息"
                            android:textColor="#FFFFFF"
                            android:textSize="14sp"
                            android:textStyle="bold" />
                    </RelativeLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginRight="5dp"
                        android:layout_weight="3"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:orientation="horizontal">
                            <TextView
                                android:id="@+id/tenderPublic_text"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="招标公告"
                                android:textColor="@color/white"
                                android:textSize="13sp"
                                android:textStyle="bold"
                                />
                            <View
                                android:layout_width="1dp"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="6dp"
                                android:layout_marginTop="8dp"
                                android:background="@color/homenew_block_line"/>
                            <TextView
                                android:id="@+id/changePublic_text"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="变更公告"
                                android:textColor="@color/white"
                                android:textSize="13sp"
                                android:textStyle="bold"
                                />
                            <View
                                android:layout_width="1dp"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="6dp"
                                android:layout_marginTop="8dp"
                                android:background="@color/homenew_block_line"/>
                            <TextView
                                android:id="@+id/getPublci_text"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="中标公告"
                                android:textColor="@color/white"
                                android:textSize="13sp"
                                android:textStyle="bold"
                                />
                        </LinearLayout>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="10dp"
                            android:background="@color/homenew_block_line"/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:orientation="horizontal">
                            <TextView
                                android:id="@+id/publicPeople_text"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="中标候选人公示"
                                android:textColor="@color/white"
                                android:textSize="13sp"
                                android:textStyle="bold"
                                />
                            <View
                                android:layout_width="1dp"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="8dp"
                                android:layout_marginTop="6dp"
                                android:background="@color/homenew_block_line"/>
                            <TextView
                                android:id="@+id/tenderSubject_text"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="招标信息订阅"
                                android:textColor="@color/white"
                                android:textSize="13sp"
                                android:textStyle="bold"
                                />
                        </LinearLayout>
                    </LinearLayout>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="83dp"
                    android:layout_marginTop="10dp"
                    android:background="@mipmap/hbg4"
                    android:orientation="horizontal">

                    <RelativeLayout
                        android:id="@+id/buyProjectInfo_layout"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1.1"
                        android:clickable="true"
                        android:gravity="center">
                        <ImageView
                            android:id="@+id/buyProjectImage"
                            android:layout_width="54dp"
                            android:layout_height="54dp"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="4dp"
                            android:src="@mipmap/caigou"
                            />
                        <TextView
                            android:id="@+id/buyInfo_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/buyProjectImage"
                            android:layout_centerHorizontal="true"
                            android:layout_marginBottom="2dp"
                            android:text="采购信息"
                            android:textColor="#FFFFFF"
                            android:textSize="14sp"
                            android:textStyle="bold"/>
                    </RelativeLayout>
                    <View
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="8dp"
                        android:layout_marginTop="8dp"
                        android:background="@color/homenew_block_line"/>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginRight="5dp"
                        android:layout_weight="3"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:orientation="horizontal">
                            <TextView
                                android:id="@+id/governmentBuy_text"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="政府采购"
                                android:textColor="@color/white"
                                android:textSize="13sp"
                                android:textStyle="bold"
                                />
                            <View
                                android:layout_width="1dp"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="6dp"
                                android:layout_marginTop="8dp"
                                android:background="@color/homenew_block_line"/>
                            <TextView
                                android:id="@+id/companyBuy_text"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="企业采购"
                                android:textColor="@color/white"
                                android:textSize="13sp"
                                android:textStyle="bold"
                                />
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="10dp"
                            android:background="@color/homenew_block_line"/>

                        <TextView
                            android:id="@+id/buyInfoSubject_text"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="采购信息订阅"
                            android:textColor="@color/white"
                            android:textSize="13sp"
                            android:textStyle="bold"
                            />
                    </LinearLayout>
                </LinearLayout>
                <TextView
                    android:id="@+id/newInfo_text"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="20dp"
                    android:text="最新信息"
                    android:textColor="#333333"
                    android:textSize="15sp"
                    />
                </LinearLayout>
            <LinearLayout
                android:id="@+id/recyclerViewContainer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="12dp"
                android:layout_marginRight="12dp"
                android:orientation="vertical">
                <ImageView
                    android:id="@+id/netErrorView"
                    android:layout_width="80dp"
                    android:layout_height="75dp"
                    android:visibility="gone"
                    android:layout_gravity="center_horizontal"
                    android:background="@mipmap/img_net_error_show"/>
        </LinearLayout>
        </LinearLayout>
    </com.zyl_android.tenderinfo.project.builder.ObservableScrollView>
    <include
        layout="@layout/item_home_search"
        />

</RelativeLayout>
